# ConnectShare Frontend Environment Variables

# Contract Addresses (Replace with your actual deployed addresses)
REACT_APP_CURTIS_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
REACT_APP_APECHAIN_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

# API Endpoints
REACT_APP_MOBILE_MONEY_API_URL=http://localhost:3001
REACT_APP_BACKEND_API_URL=http://localhost:3000

# Network Configuration
REACT_APP_DEFAULT_NETWORK=curtis
REACT_APP_ENABLE_TESTNET=true

# Performance Settings
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_CACHE_DURATION=300000

# Feature Flags
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_ANALYTICS=false
