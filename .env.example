# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Ape Chain RPC URLs
APECHAIN_RPC_URL=https://apechain.calderachain.xyz/http
APECHAIN_TESTNET_RPC_URL=https://curtis.apechain.calderachain.xyz/http

# API Keys for contract verification
APECHAIN_API_KEY=your_apechain_api_key
APECHAIN_TESTNET_API_KEY=your_apechain_testnet_api_key

# ApeCoin contract address (use appropriate address for your network)
# Mainnet: 0x4d224452801ACEd8B2F0aebE155379bb5D594381
# Testnet: deploy your own or use a mock
APE_TOKEN_ADDRESS=0x4d224452801ACEd8B2F0aebE155379bb5D594381

# Gas reporting
REPORT_GAS=true

# Coinmarketcap API key for gas reporting
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key
